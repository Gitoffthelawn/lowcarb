{
  "browser_action": {
    "browser_style": true,
    "default_title": "Remove cookies",
    "default_popup": "settings/settings.html",
    "default_icon": {
      "32": "icons/scale-32.png",
      "64": "icons/scale-64.png"
    }
  },

  "background": {
    "scripts": ["background/background.js"]
  },

  "description": "Remove unwanted cookies",
  "icons": {
    "16": "icons/scale-16.png",
    "32": "icons/scale-32.png",
    "64": "icons/scale-64.png"
  },
  "homepage_url": "https://github.com/simomat/lowcarb",
  "manifest_version": 2,
  "name": "lowcarb",
  "version": "0.1",
  "permissions": ["cookies", "<all_urls>", "storage"]
}