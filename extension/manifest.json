{
  "name": "lowcarb",
  "description": "Remove unwanted cookies",
  "homepage_url": "https://github.com/simomat/lowcarb",
  "author": "Matthias Simon",
  "version": "1.0.1",
  "manifest_version": 2,
  "permissions": ["cookies", "<all_urls>", "storage"],
  "applications": {
    "gecko": {
      "id": "lowcab@infonautika.de",
      "strict_min_version": "52.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "icons": {
    "16": "icons/scale-16.png",
    "32": "icons/scale-32.png",
    "64": "icons/scale-64.png"
  },

  "options_ui": {
    "page": "options/options.html"
  }
}